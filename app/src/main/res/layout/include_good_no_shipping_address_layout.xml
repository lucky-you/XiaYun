<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_good_no_have_address_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:visibility="gone"
    android:background="@drawable/ic_ander_img_small"
    android:orientation="horizontal">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/spacing_24"
        android:src="@drawable/ic_order_fi_one" />

    <TextView
        android:id="@+id/tv_no_select_address"
        style="@style/TextViewStyleOf14Sp"
        android:layout_centerInParent="true"
        android:text="@string/The_shipping_address_is_not_filled_in"
        android:textColor="@color/color_9f9f9f_black" />


</RelativeLayout>
